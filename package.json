{
  "name": "vue-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "format:all": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose build",
    "docker:dev:detach": "docker-compose up -d",
    "docker:test": "docker-compose -f docker/test/docker-compose.yml up",
    "docker:test:build": "docker-compose -f docker/test/docker-compose.yml build",
    "docker:test:detach": "docker-compose -f docker/test/docker-compose.yml up -d",
    "docker:test:bg": "./scripts/start-test-background.sh",
    "docker:prod": "docker-compose -f docker/prod/docker-compose.yml up",
    "docker:prod:build": "docker-compose -f docker/prod/docker-compose.yml build",
    "docker:prod:detach": "docker-compose -f docker/prod/docker-compose.yml up -d",
    "docker:deploy-prod": "./scripts/run-prod.sh",
    "docker:dev:sh": "docker exec -it vue-dev bash",
    "docker:test:sh": "docker exec -it vue-test sh",
    "docker:prod:sh": "docker exec -it vue-prod sh",
    "docker:clean:dev": "docker-compose down -v",
    "docker:clean:test": "docker-compose -f docker/test/docker-compose.yml down -v",
    "docker:clean:prod": "docker-compose -f docker/prod/docker-compose.yml down -v",
    "docker:clean:all": "npm run docker:clean:dev && npm run docker:clean:test && npm run docker:clean:prod"
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "~10.0.0",
    "jsdom": "^22.1.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^1.2.0",
    "vue-tsc": "^2.2.8"
  }
}