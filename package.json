{
  "name": "vue-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "format:all": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "docker:dev:start": "chmod +x ./docker/dev/start.sh && ./docker/dev/start.sh",
    "docker:dev:restart": "chmod +x ./docker/dev/restart.sh && ./docker/dev/restart.sh",
    "docker:dev:stop": "chmod +x ./docker/dev/stop.sh && ./docker/dev/stop.sh",
    "docker:test:start": "chmod +x ./docker/test/start.sh && ./docker/test/start.sh",
    "docker:test:restart": "chmod +x ./docker/test/restart.sh && ./docker/test/restart.sh",
    "docker:test:stop": "chmod +x ./docker/test/stop.sh && ./docker/test/stop.sh",
    "docker:prod:start": "chmod +x ./docker/prod/start.sh && ./docker/prod/start.sh",
    "docker:prod:restart": "chmod +x ./docker/prod/restart.sh && ./docker/prod/restart.sh",
    "docker:prod:stop": "chmod +x ./docker/prod/stop.sh && ./docker/prod/stop.sh",
    "docker:start:all": "chmod +x ./start-all.sh && ./start-all.sh",
    "docker:restart:all": "chmod +x ./restart-all.sh && ./restart-all.sh", 
    "docker:stop:all": "chmod +x ./stop-all.sh && ./stop-all.sh",
    "docker:clean": "chmod +x ./clean.sh && ./clean.sh",
    "docker:dev:sh": "docker exec -it vue-dev bash",
    "docker:test:sh": "docker exec -it vue-test sh",
    "docker:prod:sh": "docker exec -it vue-prod sh"
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "~10.0.0",
    "jsdom": "^22.1.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^1.2.0",
    "vue-tsc": "^2.2.8"
  }
}